				<ProtectedString name="Source"><![CDATA[sp=script.Parent
plr=game.Players.localPlayer
local ChangePos = sp:WaitForChild("ChangePos")

local MultiFire = false
range = 975
firerate = 1.2

local CurGui
equipped=false
check=true

function checkintangible(hit)
	if hit and hit~=nil then
		if hit:IsDescendantOf(sp.Parent) or hit.Transparency>.8 or hit.Name=="Handle" or hit.Name == "NoobPart" or hit.Name == "NoobTorso" or hit.Name=="Effect" or hit.Name=="Bullet" or hit.Name=="Laser" or string.lower(hit.Name)=="water" or hit.Name=="Rail" then
			return true
		end
	end
	return false
end

function castray(startpos,vec,length,ignore,delayifhit)
	local hit,endpos2=game.Workspace:FindPartOnRay(Ray.new(startpos,vec*length),ignore)
	if hit~=nil then
		if checkintangible(hit) then
			if delayifhit then
				wait()
			end
			hit,endpos2=castray(endpos2+(vec*.01),vec,length-((startpos-endpos2).magnitude),ignore,delayifhit)
		end
	end
	return hit,endpos2
end
local Ammo,AmmoMode,ImpactMode = "Rail","Normal","Damage"
local function Fire(mouse)
	local he=sp.Parent:FindFirstChild("Head")
	local hu=sp.Parent:FindFirstChild("Humanoid")
	if check and he and hu and hu.Health>0 then
		check = false
		mouse.Icon="rbxasset://textures\\GunWaitCursor.png"
		local startpos=he.Position
		local vec=(mouse.Hit.p-startpos).unit
		local hit,endpos=castray(startpos,vec,range,sp.Parent,false)
		script.Parent.Fire:FireServer(vec,hit,endpos)
		if script.Parent.FireMode.Value == "Normal" then
			wait(firerate)
		else
			wait(0.1)
		end
		mouse.Icon="rbxasset://textures\\GunCursor.png"
		check=true
	end
end


function onEquipped(mouse)
	equipped=true
	if mouse~=nil then
		mouse.Icon="rbxasset://textures\\GunCursor.png"
		CurGui = script.Parent.GunGui:Clone()
		CurGui.Parent = game.Players.LocalPlayer.PlayerGui
		local MainGui = CurGui.MainGui
		
		local function UpdateButtons()
			MainGui.Ammo1.Style = (Ammo == "Rail" and "RobloxButtonDefault" or "RobloxButton")
			MainGui.Ammo2.Style = (Ammo == "Knife" and "RobloxButtonDefault" or "RobloxButton")
			MainGui.Ammo3.Style = (Ammo == "Noob" and "RobloxButtonDefault" or "RobloxButton")
			
			MainGui.Mode1.Style = (AmmoMode == "Normal" and "RobloxButtonDefault" or "RobloxButton")
			MainGui.Mode2.Style = (AmmoMode == "Laser" and "RobloxButtonDefault" or "RobloxButton")
			MainGui.Mode3.Style = (AmmoMode == "MLGLaser" and "RobloxButtonDefault" or "RobloxButton")
			
			MainGui.Impact1.Style = (ImpactMode == "Damage" and "RobloxButtonDefault" or "RobloxButton")
			MainGui.Impact2.Style = (ImpactMode == "Nothing" and "RobloxButtonDefault" or "RobloxButton")
			MainGui.Impact3.Style = (ImpactMode == "Explode" and "RobloxButtonDefault" or "RobloxButton")
			MainGui.Impact4.Style = (ImpactMode == "ExplodeNoDamage" and "RobloxButtonDefault" or "RobloxButton")
			
			MainGui.FMode1.Style = (MultiFire == false and "RobloxButtonDefault" or "RobloxButton")
			MainGui.FMode2.Style = (MultiFire == true and "RobloxButtonDefault" or "RobloxButton")
		end
		
		local DB = true
		local function IntButtonClick(Button,Type,Value)
			Button.MouseButton1Down:connect(function()
				if DB == true then
					DB = false
					if Type == "Ammo" then
						Ammo = Value
						script.Parent.ChangeMode:FireServer("Ammo",Value)
					elseif Type == "AmmoMode" then
						AmmoMode = Value
						script.Parent.ChangeMode:FireServer("AmmoMode",Value)
					elseif Type == "ImpactMode" then
						ImpactMode = Value
						script.Parent.ChangeMode:FireServer("ImpactType",Value)
					elseif Type == "MultiFire" then
						MultiFire = Value
					end
					UpdateButtons()
					wait()
					DB = true
				end
			end)
		end
		
		
		IntButtonClick(MainGui.Ammo1,"Ammo","Rail")
		IntButtonClick(MainGui.Ammo2,"Ammo","Knife")
		IntButtonClick(MainGui.Ammo3,"Ammo","Noob")
		IntButtonClick(MainGui.Mode1,"AmmoMode","Normal")
		IntButtonClick(MainGui.Mode2,"AmmoMode","Laser")
		IntButtonClick(MainGui.Mode3,"AmmoMode","MLGLaser")
		IntButtonClick(MainGui.Impact1,"ImpactMode","Damage")
		IntButtonClick(MainGui.Impact2,"ImpactMode","Nothing")
		IntButtonClick(MainGui.Impact3,"ImpactMode","Explode")
		IntButtonClick(MainGui.Impact4,"ImpactMode","ExplodeNoDamage")
		IntButtonClick(MainGui.FMode1,"MultiFire",false)
		IntButtonClick(MainGui.FMode2,"MultiFire",true)
		
		local Open = false
		MainGui.Open.MouseButton1Down:connect(function()
			if DB == true then
				DB = false
				if Open == false then
					Open = true
					MainGui:TweenPosition(UDim2.new(1,-370,1,-150),"InOut","Quad",0.25,true)
					MainGui.Open.Text = ">"
				else
					Open = false
					MainGui:TweenPosition(UDim2.new(1,0,1,-150),"InOut","Quad",0.25,true)
					MainGui.Open.Text = "<"
				end
				DB = true
			end
		end)
		
		
		local Down = false
		local he = sp.Parent:FindFirstChild("Head")
		local hu = sp.Parent:FindFirstChild("Humanoid")
		mouse.Button1Down:connect(function()
			if DB == true then
				DB = false
				if MultiFire == false then
					Fire(mouse)
				else
					Down = true
					while hu.Health > 0 and Down == true and script.Parent.CanFire.Value == true do 
						Fire(mouse)
					end
				end
				DB = true
			end
		end)
		mouse.Button1Up:connect(function()
			if Down == true then
				Down = false
			end
		end)
		
		UpdateButtons()
	end
end

function onUnequipped()
	equipped=false
	if CurGui then CurGui:Destroy() end
end

sp.Equipped:connect(onEquipped)
sp.Unequipped:connect(onUnequipped)

local DB = true
game:GetService("UserInputService").InputBegan:connect(function(Key)
	
	if DB == true and equipped == true then
		DB = false
		if Key.KeyCode == Enum.KeyCode.Q then
			ChangePos:FireServer("Q")
			wait(0.5)
		end
		DB = true
	end
end)




